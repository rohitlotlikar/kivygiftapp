<MainWindow>:
    canvas.before:
        Color:
            rgba: 0.549, .8549, 1, 1
        Rectangle:
            size: self.width,self.height
    name:"main"
    FloatLayout:
        
        Label:
            text:"Welcome"
            color: 0.949,.46667,.957255,0.8
            font_name:'alex'
            size_hint:0.5,0.2
            pos_hint:{"x":0.25,"top":0.9}
            font_size: (root.width**2 + root.height**2)*2 / 11**4
        Label:
            text:"To gifty"
            color: 0.949,.46667,.957255,0.8
            size_hint:0.3,0.1
            font_name:'alex'
            pos_hint:{"x":0.35,"top":0.6}
            font_size: (root.width**2 + root.height**2) / 10**4
        Button:
            text:"ENTER"
            font_name:'great'
            color:0.549, .8549, 1, 1
            background_color:0.949,.46667,.957255,0.7
            size_hint:0.8,0.19
            pos_hint:{"x":0.1,"top":0.3}
            font_size: (root.width**2 + root.height**2) / 12**4           
            on_release:
                root.next()
                root.manager.transition.direction="left"
                
                

